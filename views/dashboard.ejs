}
.create-flashcard {
    box-shadow: -4px -4px 8px #222, -3px -3px 6px #111, 6px 6px 8px #111,  3px 3px 6px #000, inset -2px -2px 2px #222, inset 2px 2px 2px #222;
    border-radius: 15px;
    padding: 20px;
    margin-bottom: 30px;
}
.create-flashcard input, .create-learning-point input {
    display: block;
    width: 100%;
    padding: 12px;
    margin-bottom: 10px;
    border-radius: 10px;
    box-shadow: inset -3px -3px 6px #000, inset 3px 3px 6px #111, inset 3px 3px 6px #333, inset -3px -3px 6px #111;
    background: #111;
    border: none;
    outline: none;
    transition: ease-in 0.5s;
}
.create-flashcard input:hover, .create-learning-point input:hover {
    box-shadow: inset -3px -3px 6px #333, inset 3px 3px 6px #111, inset 3px 3px 6px #333, inset -3px -3px 6px #111;
    background: #222;
    transition: ease-in 0.3s;
}
.create-flashcard input:focus, .create-learning-point input:focus {
    box-shadow: inset 0 0 2px rgba(135, 206, 250, 0.89), inset 0 0 12px rgba(135, 206, 250, 0.603), inset -3px -3px 6px #333, inset 3px 3px 6px #111, inset 3px 3px 6px #333, inset -3px -3px 6px #111;
    background: #333;
    transition: ease-in 0.3s;
}
.create-learning-point {
    box-shadow: -4px -4px 8px #222, -3px -3px 6px #111, 6px 6px 8px #111,  3px 3px 6px #000, inset -2px -2px 2px #222, inset 2px 2px 2px #222;
    border-radius: 15px;
    padding: 20px;
    margin-bottom: 30px;
}
.create-learning-point textarea {
    margin-bottom: 10px;
    min-height: 150px;
    padding: 12px;
    border-radius: 20px;
    box-shadow: inset -3px -3px 6px #000, inset 3px 3px 6px #111, inset 3px 3px 6px #333, inset -3px -3px 6px #111;
    background: #111;
    transition: ease-in 0.5s;
    outline: none;
    border: none;
    color: #fff;
    width: 100%;
}
.create-learning-point textarea:hover {
    box-shadow: inset -3px -3px 6px #333, inset 3px 3px 6px #111, inset 3px 3px 6px #333, inset -3px -3px 6px #111;
    background: #222;
    transition: ease-in 0.3s;
}
.create-learning-point textarea:focus {
    background: #333;
    box-shadow: inset 0 0 2px rgba(135, 206, 250, 0.89), inset 0 0 12px rgba(135, 206, 250, 0.603), inset -3px -3px 6px #000, inset 3px 3px 6px #111, inset 3px 3px 6px #333, inset -3px -3px 6px #111;
    transition: ease-in 0.3s;
}

