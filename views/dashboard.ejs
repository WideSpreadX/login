/* Dropdown */
.dropdown {
    position: relative;
    display: inline-block;
}

.dropdown-content {
    display: none;
    position: absolute;
    min-width: 400px;
    z-index: 1;
    border: solid 1px lightskyblue;
    box-shadow: inset 0 0 30px lightskyblue, -3px -3px 6px #222, -3px -3px 6px #111, 3px 3px 3px #111,  3px 3px 6px #000, inset 0 0 10px #ccc, inset 4px 4px 8px #000, inset 3px 3px 6px #111, inset -6px -6px 8px #222, inset -3px -3px 4px #444, inset 2px 2px 2px #333, inset -2px -2px 2px #222;
    background: rgba(0, 0, 0, 0.377);
    backdrop-filter: blur(1px);
    border-radius: 20px;
    transition: ease-in 0.3s;
}
.dropdown-content a {
    color: black;
    background: transparent;
    padding: 12px 16px;
    text-decoration: none;
    display: block;
    z-index: 2;
    transition: ease-in 0.3s;
}
.dropdown-content form {
    color: black;
    background: transparent;
    padding: 12px 16px;
    text-decoration: none;
    display: block;
    z-index: 2;
    transition: ease-in 0.3s;
}
.dropdown-content>.add-form>input {
    background: rgba(255, 255, 255, 0.774);
}
.dropdown:hover .dropdown-content {
    display: block;
}
.dropdown-menu {
    position: relative;
    display: inline-block;
}
.dropbtn-menu {
    background: transparent;
    box-shadow: inset 0 0 10px lightskyblue, -4px -4px 8px #222, -3px -3px 6px #111, 6px 6px 8px #111,  3px 3px 6px #000, inset -2px -2px 2px #222, inset 2px 2px 2px #222;
    border-bottom-left-radius: 50%;
    border-bottom-right-radius: 50%;
    border-radius: 50%;
    width: 50px;
    height: 50px;
    outline: none;
    border: none;
    transition: ease-in 0.1s;
}

.dropdown-content-menu {
    display: none;
    position: absolute;
    min-width: 100px;
    z-index: 1;
    border: solid 1px lightskyblue;
    box-shadow: inset 0 0 30px lightskyblue, 0 0 10px lightskyblue;
    background: rgba(0, 0, 0, 0.377);
    backdrop-filter: blur(1px);
    border-radius: 20px;
    transition: ease-in 0.1s;
}
.dropbtn-menu:hover {
    border-radius: 0;
    border-top-left-radius: 50%;
    border-top-right-radius: 50%;
    transition: ease-in 0.1s;
}
.dropdown-content-menu button {
    box-shadow: -4px -4px 8px #222, -3px -3px 6px #111, 6px 6px 8px #111,  3px 3px 6px #000, inset -2px -2px 2px #222, inset 2px 2px 2px #222;
    background: transparent;
}
.dropdown-content-menu button:active {
    outline: none;
}
.dropdown-menu:hover .dropdown-content-menu {
    display: block;
    animation-delay: 0.1s;
}
.dropdown-content-menu a {
    color: rgba(204, 204, 204, 0.699);
    background: transparent;
    padding: 12px 16px;
    text-decoration: none;
    display: block;
    z-index: 1;
    font-size: 2rem;
    transition: ease-in 0.5s;
}
.dropdown-content-menu a:hover {
    color: #fff;
    text-shadow: 0 0 3px lightskyblue;
    background: transparent;
    padding: 12px 16px;
    text-decoration: none;
    display: block;
    z-index: 2;
    transition: ease-in 0.3s;
}
